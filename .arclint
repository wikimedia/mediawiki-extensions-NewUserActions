{
  "exclude": [
    "(^vendor/)"
  ],
  "linters": {
    "chmod": {
      "type": "chmod"
    },
    "json": {
      "type": "json",
      "include": [
        "(^\\.arcconfig$)",
        "(^\\.arclint$)",
        "(\\.json$)"
      ]
    },
    "merge-conflict": {
      "type": "merge-conflict"
    },
    "php": {
      "type": "php",
      "include": "(\\.php$)"
    }
  }
}
